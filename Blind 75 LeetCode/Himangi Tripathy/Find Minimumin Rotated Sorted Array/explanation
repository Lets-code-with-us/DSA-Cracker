class Solution {
    public int findMin(int[] nums) {
        int minele=nums[0];//here we initialised minele to 1st element of nums array
        for(int i=0;i<nums.length;i++){//here we traverse nums array to find minimum element
            if(nums[i]<minele){//we compare nums array element to minele if minele is bigger than nums[i] then
                minele=nums[i];// we assign nums[i] to minele
            }
        }
        return minele;//we return minele
        
    }
}